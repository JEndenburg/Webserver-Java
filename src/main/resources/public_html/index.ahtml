<HTML>
    <head>
        <link rel="stylesheet" href="style/main.css">
        <title>Welcome to my site!</title>
    </head>
    <body>
        <header>
            <div id="sitename">My Site!</div>
        </header>
        <main>
            <article>
                <div class="article-title">This is an article!</div>
                <div class="article-content">

                    <img src="img/cat.jpg">
                    So yeah, welcome to my site. My name is DJ Fluffypuffkins.

                    <%

                        DEFINE INT $myVal
                        GIVE_STATIC $myVal "Thing"

                        ISNULL $myVal
                            SET $myVal 0

                        PRINT $myVal

                        ADD $myVal 1
                        TAKE_STATIC $myVal "Thing"
                    %>
                </div>
            </article>
        </main>
    </body>
</HTML>